{
  "version": 2,
  "routes": [
    { "src": "/admin-signup.js", "dest": "/public/admin-signup.js" },
    { "src": "/styles.css", "dest": "/public/styles.css" },
    { "src": "/login.js", "dest": "/public/login.js" },
    { "src": "/car-company/(app\\.js|styles\\.css)", "dest": "/public/car-company/$1" },
    { "src": "/insurance-company/(app\\.js|styles\\.css)", "dest": "/public/insurance-company/$1" },
    { "src": "/admin-signup(?:/.*)?", "dest": "/public/admin-signup/index.html" },
    { "src": "/car-company(?:(?!/app\\.js|/styles\\.css).*)?", "dest": "/public/car-company/index.html" },
    { "src": "/insurance-company(?:(?!/app\\.js|/styles\\.css).*)?", "dest": "/public/insurance-company/index.html" },
    { "src": "/public/(.*)", "dest": "/public/$1" },
    { "src": "/(?:index\\.html)?", "dest": "/public/index.html" }
  ]
}
